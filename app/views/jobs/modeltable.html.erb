<h1>Government Job Listings</h1>

<table>
  <thead>
    <tr>
      <th>Government ID</th>
      <th>Position Title</th>
      <th>Organization Name</th>
      <th>Rate Interval Code</th>
	  <th>Minimum</th>
      <th>Maximum</th>
      <th>Start Date</th>
      <th>End Date</th>
      <th>Locations</th>
      <th>URL</th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.govt_id %></td>
        <td><%= job.position_title %></td>
        <td><%= job.organization_name %></td>
        <td><%= job.rate_interval_code %></td>
        <td><%= job.minimum %></td>
        <td><%= job.maximum %></td>
        <td><%= job.start_date %></td>
        <td><%= job.end_date %></td>
        <td><%= job.locations %></td>
        <td><%= job.url %></td>
      </tr>
    <% end %>
  </tbody>
</table>
