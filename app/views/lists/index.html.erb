<h1>Listing lists</h1>
<% get_jobs %>
<table>
  <thead>
    <tr>
      <th>Govt</th>
      <th>Position</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Position title</th>
      <th>Organization name</th>
      <th>Rate interval code</th>
      <th>Minimum</th>
      <th>Maximum</th>
      <th>Locations</th>
      <th>Url</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= list.govt_id %></td>
        <td><%= list.position %></td>
        <td><%= list.start_date %></td>
        <td><%= list.end_date %></td>
        <td><%= list.position_title %></td>
        <td><%= list.organization_name %></td>
        <td><%= list.rate_interval_code %></td>
        <td><%= list.minimum %></td>
        <td><%= list.maximum %></td>
        <td><%= list.locations %></td>
        <td><%= list.url %></td>
        <td><%= link_to 'Show', list %></td>
        <td><%= link_to 'Edit', edit_list_path(list) %></td>
        <td><%= link_to 'Destroy', list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New List', new_list_path %>
